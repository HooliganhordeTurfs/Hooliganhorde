{
  "name": "subgraph-hooliganhorde",
  "license": "MIT",
  "description": "A subgraph for indexing events emitted by Hooliganhorde.",
  "repository": {
    "type": "git",
    "url": "https://github.com/HooliganhordeGangs/Hooliganhorde.git",
    "directory": "projects/subgraph-hooliganhorde"
  },
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "test": "graph test",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ hooliganhorde",
    "create-local": "graph create --node http://127.0.0.1:8020/ hooliganhorde",
    "deploy-local": "graph deploy --node http://127.0.0.1:8020/ --ipfs http://127.0.0.1:5001 hooliganhorde",
    "create-hosted": "graph create --node http://graph.node.hooligan.black:8020/ hooliganhorde",
    "remove-hosted": "graph remove --node http://graph.node.hooligan.black:8020/ hooliganhorde",
    "deploy-hosted": "graph deploy --node http://graph.node.hooligan.black:8020/ --ipfs http://graph.node.hooligan.black:5001 hooliganhorde",
    "create-hosted-dev": "graph create --node http://graph.node.hooligan.black:8020/ hooliganhorde-dev",
    "remove-hosted-dev": "graph remove --node http://graph.node.hooligan.black:8020/ hooliganhorde-dev",
    "deploy-hosted-dev": "graph deploy --node http://graph.node.hooligan.black:8020/ --ipfs http://graph.node.hooligan.black:5001 hooliganhorde-dev",
    "create-hosted-test": "graph create --node http://graph.node.hooligan.black:8020/ hooliganhorde-testing",
    "remove-hosted-test": "graph remove --node http://graph.node.hooligan.black:8020/ hooliganhorde-testing",
    "deploy-hosted-test": "graph deploy --node http://graph.node.hooligan.black:8020/ --ipfs http://graph.node.hooligan.black:5001 hooliganhorde-testing"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.30.4",
    "@graphprotocol/graph-ts": "^0.27.0",
    "matchstick-as": "^0.5.0"
  },
  "private": true
}
